package OOPs;

public class Static {
	
	int id;
	String name;
	static String college;
	
	void list(int i,String n)
	{
		id=i;
		name=n;
		//college=c;
	}
	void display()
	{
		System.out.println(id+" "+name+" "+college);
	}

	public static void main(String[] args) {
		
		Static s=new Static();
		
		s.display();
		
		
	}

}
