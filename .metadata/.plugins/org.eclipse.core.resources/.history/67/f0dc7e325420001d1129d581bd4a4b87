package OOPs;



public class Student {
	
	public void add(int a,int b)
	{
		System.out.println(a+b);
	}
	
	public void add(int a,int b,int c)
	{
		System.out.println(a+b+c);
	}
	
	public static void main(String[] args) {
		
		Student s=new Student();
		
		s.add(10, 10);
		s.add(10, 10, 10);
	
		
		
		
		
	}

}
