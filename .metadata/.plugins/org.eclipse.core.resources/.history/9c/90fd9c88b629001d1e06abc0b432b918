package OOPs;

public class Student {
	
	int id;
	String name;
	float fee;
	
	void detail(int i,String n)
	{
		id=i;
		name=n;
	}
}
class College extends Student{
	
	void show(float f)
	{
		fee=f;
	}
	

	public static void main(String[] args) {
		
		College c=new College();
		
		c.detail(101, "julie");
		c.show(200);
		
		System.out.println(c.id+" "+c.name+" "+c.fee);
		
		
		
		
	}

}
