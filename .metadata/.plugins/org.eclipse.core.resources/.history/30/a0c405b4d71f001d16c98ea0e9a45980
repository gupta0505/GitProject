package OOPs;



public class Student {
	int id;
	String name;
	
	
	
	public static void main(String[] args) {
		
		Student s=new Student();
		Student s1=new Student();
		
		s.id=101;
		s.name="Julie";
		
		s1.id=102;
		s1.name="Deepak";
		
		System.out.println(s.id+" "+s.name);
		System.out.println(s1.id+" "+s1.name);
		
		
		
		
		
		
		
	}

}
