package OOPs;

import java.net.IDN;

public class Student {
	
	int id;
	String name;
	static String school="Raisoni";
	
	static void change()
	{
		school="Ycc";
	}
	
	Student(int i,String n)
	{
		id=i;
		name=n;
	}
	
	void display()
	{
	System.out.println(id+" "+name+" "+school);
	}
	
	
	public static void main(String[] args) {
		
		Student s=new Student(101,"Julie");
		Student s1=new Student(102,"Deepak");
		
		s.display();
		s1.display();
		
		
		
		
		
		
	}

}
