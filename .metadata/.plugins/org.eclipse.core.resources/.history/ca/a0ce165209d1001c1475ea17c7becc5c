package testPackage;

public class FirstProgram {

	public static void main(String[] args) {
		
		
		System.out.println("Hello World");
		System.out.println("it will be add new line for each method");
		
		
		String name="Deepak";
		System.out.println(name);
		
		
		int myNum=15;
		System.out.println(myNum);
		
		
		int number=15;
		number=20;    //number is now 20
		System.out.println(number);
		
		
		final int myNo=15;
		myNo=20;    // will generate an error
		System.out.println(myNo);
		
		

	}

}
