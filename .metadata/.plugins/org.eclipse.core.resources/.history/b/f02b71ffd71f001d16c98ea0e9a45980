package OOPs;



public class Student {
	int id;
	String name;
	
	void detail(int i,String n)
	{
		id=i;
		name=n;
	}
	
	void display()
	{
		System.out.println(id+" "+name);
	}
	
	public static void main(String[] args) {
		
		Student s=new Student();
		Student s1=new Student();
		
		s.detail(101, "Julie");
		s1.detail(102, "Deepak");
		
		s.display();
		s1.display();
		
		
		
		
		
		
		
		
	}

}
