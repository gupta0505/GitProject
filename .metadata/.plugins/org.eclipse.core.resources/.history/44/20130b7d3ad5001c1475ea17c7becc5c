package testPackage;

public class DataTypes2 {

	public static void main(String[] args) {
		
		//FLOATING POINT DATATYPES
		
		//float- 
		 float f=2.6767765439876544676877878778787877676766887878787878787878f;
		 System.out.println(f);
		 
		 
		 //double-
		 double d=2.6767765439876544676877878778787877676766887878787878787878d;
		 System.out.println(d);
		 
		 //Boolean-
		 boolean b=true;
		 System.out.println(b);
		 
		 //char-
		 char letter='D';
		 System.out.println(letter);
		 
		 char mynum='2';
		 System.out.println(mynum);
		 
		 
	}

}
