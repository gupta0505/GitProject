package OOPs;

public class Student {
	
	private int age;
	
	public int getAge()
	{
		return age;
	}
	
	public void setAge(int age)
	{
		this.age=age;
	}
	
}

class main{

	public static void main(String[] args) {
	
		Student s=new Student();
		
		s.setAge(24);
		System.out.println(s.getAge());
		
		
		
		
		
		
	}

}
