package BASIC;

public class Practice {
	
	public static void main(String[] args) {
		
		int num=23, number,total=0,temp;
		
		number=num;
		
		while(number>0)
		{
			temp=number%10;
			total=total+temp*temp;
			number=number/10;
		}
		if(total==num)
			
			System.out.println("am");
		
		else
			
			System.out.println("not");
		}
		
	}

		 
	


