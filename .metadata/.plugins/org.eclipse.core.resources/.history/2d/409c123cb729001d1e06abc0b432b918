package OOPs;

public class Student {
	
	int id;
	String name;
	float fee;
	
	void detail(int i,String n)
	{
		id=i;
		name=n;
	}
}
class College extends Student{
	
	void show(float f)
	{
		fee=f;
	}
}
class main{
	
	public static void main(String[] args) {
		
		College s=new College();
		
		s.id=101;
		s.name="julie";
		s.fee=200;
		
		System.out.println(s.id+" "+s.name+" "+s.fee);
		
		
		
		
	}

}
