package OOPs;



public class Student {
	
		void show(int a,int b)
		{
			System.out.println(a+b);
		}
		
		void show(int a) 
		{
			System.out.println(a);
		}
		
		void show(int a,int b,int c) 
			
		{
			System.out.println(a+b+c);
		}
		
	
	public static void main(String[] args) {
		
		Student s=new Student();
		
		s.show(10);
		s.show(10, 10);
		s.show(1, 10, 10);
		
		
		
	}
}
