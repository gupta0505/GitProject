package OOPs;

public class Static {
	
	int id;
	String name;
	static String college="Raisoni";
	
	Static(int i,String n)
	{
		id=i;
		name=n;
		//college=c;
	}
	
	public static void main(String[] args) {
		
		Static s=new Static(100,"julie");
		Static s1=new Static(101,"Nisha");
		System.out.println(s.id+" "+s.name+" "+s.college);
		System.out.println(s.id+" "+s.name+" "+s.college);
		
		
	}

}
