package practice;

public class ArrayProgram {
	
		
public static int getLargest(int []arr,int n) {
	
	int temp=0;
	for(int i=0;i<arr.length;i++)
	{
		for(int j=i+1;j<arr.length;j++)
		{
			if(arr[i]>arr[j])
			{
				temp=arr[i];
				arr[i]=arr[j];
				arr[j]=temp;
			}
		}
	}
	return arr[4];
}
	public static void main(String[] args) {
		int []arr= {1,2,3,4,5,6};
		
	
	
		System.out.println(getLargest(arr,6));
	}
		
				
	}

	


