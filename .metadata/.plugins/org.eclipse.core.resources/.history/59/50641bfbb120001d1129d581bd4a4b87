package OOPs;



public class Student {
	
	int id;
	String name;
	float ratio;
	
	Student(int i,String n)
	{
		id=i;
		name=n;
	}
	
	Student (float r)
	{
		ratio=r;
	}
	
	public static void main(String[] args) {
	
		Student s=new Student(101,"julie");
		Student s1=new Student(10.10f);
		
		System.out.println(s.id+" "+s.name);
		System.out.println(s1.ratio);
		
	}
}
