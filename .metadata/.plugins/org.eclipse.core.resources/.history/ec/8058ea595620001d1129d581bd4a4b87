package OOPs;



public class Student {
	
	public void run()
	{
		System.out.println("aaaaaaaaa");
	}
}
	
	class Subject extends Student{
		
		public void run()
		{
			System.out.println("bbbbbbbbbbbb");
		}
		
	
	public static void main(String[] args) {
		
		
		Student s=new Student();
		s.run();
		
	
		
		
		
		
	}

}
