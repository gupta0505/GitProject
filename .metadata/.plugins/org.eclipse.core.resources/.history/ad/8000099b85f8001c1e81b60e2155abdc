package oopS;


public class Overloading {
	
	 void add(int a,int b)
	{
		System.out.println((a+b));
	}
	void add(int a,int b,int c)
	{
		System.out.println(a+b+c);
	}
	
	public static void main(String []args) {
	Overloading o=new Overloading();
	o.add(11, 11);
	o.add(11, 11, 11);
	
	 
	}
	
			
		
	
}

